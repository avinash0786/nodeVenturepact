<div style="margin-left: 10%; margin-bottom: 10%">
    <h1>All sent Emails </h1>

    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th scope="col">Reciever</th>
            <th scope="col">Message</th>
            <th scope="col">Date</th>
        </tr>
        </thead>
        <tbody>
    {{#each emails}}
        <tr>
            <td>{{this.reciever}}</td>
            <td>{{this.message}}</td>
            <td>{{this.date}}</td>
        </tr>
    {{/each}}
        </tbody>
    </table>
</div>